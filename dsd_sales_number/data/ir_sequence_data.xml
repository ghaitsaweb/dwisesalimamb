<?xml version="1.0" encoding="utf-8"?>
<odoo >

    <record id="new_order_running_no" model="ir.sequence">
        <field name="name">New Sales Order No</field>
        <field name="code">new.order.running.no</field>
        <field name="padding">5</field>
        <field name="prefix">SO</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="new_quotation_running_no" model="ir.sequence">
        <field name="name">New Sales Quotation No</field>
        <field name="code">new.quotation.running.no</field>
        <field name="padding">5</field>
        <field name="prefix">SQ</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>